{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "lib": ["ESNext"],
      "moduleResolution": "bundler",
      "strict": true,
      "baseUrl": "./",
      "skipLibCheck": true,
      "typeRoots": ["./node_modules/@types"],
      "types": ["node"],
      "jsx": "react-jsx",
      "jsxImportSource": "hono/jsx",
      "outDir": "./dist"
    },
    "tsc-alias": {
      "resolveFullPaths": true
    }
  }